{
  "name": "blogs-api",
  "version": "1.0.0",
  "description": "Template repository to hold linter configurations",
  "main": "index.js",
  "scripts": {
    "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate $",
    "start": "node index.js",
    "drop": "npx sequelize-cli db:drop $",
    "seed": "npx sequelize-cli db:seed:all $",
    "debug": "nodemon index.js",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CamilaCode7/Project-Blogs-Api.git"
  },
  "author": "Camila",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CamilaCode7/Project-Blogs-Api/issues"
  },
  "homepage": "https://github.com/CamilaCode7/Project-Blogs-Api#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "eslint-config-trybe-backend": "^1.0.4",
    "express": "^4.17.1",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.12.0-beta.1"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.3.0"
  }
